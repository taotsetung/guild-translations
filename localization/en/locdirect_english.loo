		{
			Key = "$BatBlood";
			Text = "Bat blood";
		},
		LocDirectEntry
		{
			Key = "$BatBloodDesc";
			Text = "Bat blood is a particularly strong dye. It is taken from the insidious vampire bats which make the cemeteries spooky places at night.";
		},
